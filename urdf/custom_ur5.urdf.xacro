        <origin xyz="8.625E-08 -4.6583E-06 0.03145" rpy="0 0 0" />
        <mass value="10.22652" />
        <inertia ixx="0.020005" ixy="-4.2442E-10" ixz="-2.9069E-10" iyy="0.17832" iyz="-3.4402E-08" izz="0.013478" />
      </inertial> -->
      <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.48" />
      <inertia
        ixx="6.66"
        ixy="0"
        ixz="0"
        iyy="6.66"
        iyz="0"
        izz="6.66" />
    </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
        <!-- <box size="0.05 0.05 0.05"/> -->
          <!-- <mesh filename="package://ur5_intpro/meshes/visual/robotiq_arg2f_85_base_link_1.dae"/> -->
          <mesh filename="package://ur5_intpro/meshes/visual/base.dae"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
        <!-- <box size="0.05 0.05 0.05"/> -->
           <!-- <mesh filename="package://ur5_intpro/meshes/visual/robotiq_arg2f_85_base_link_1.dae" /> -->
           <mesh filename="package://ur5_intpro/meshes/visual/base.dae" />
        </geometry>
      </collision>
    </link>

  <joint name="robotiq_joint" type="fixed">
    <parent link="tool0" />
    <child link = "robotiq_arg2f_base_link" />
    <origin xyz="0.5 0.0 0.5" rpy="0.0 0.0 0.0" />
 </joint>

